{
	"name": "DFPaysim",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DestinationPaysim",
						"type": "DatasetReference"
					},
					"name": "Paysim"
				}
			],
			"sinks": [
				{
					"name": "PaysimSQL"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          step as integer,",
				"          type as string,",
				"          amount as double,",
				"          nameOrig as string,",
				"          oldbalanceOrg as double,",
				"          newbalanceOrig as double,",
				"          nameDest as string,",
				"          oldbalanceDest as double,",
				"          newbalanceDest as double,",
				"          isFlaggedFraud as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> Paysim",
				"Paysim sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          step,",
				"          type,",
				"          amount,",
				"          nameOrig,",
				"          oldbalanceOrig = oldbalanceOrg,",
				"          newbalanceOrig,",
				"          nameDest,",
				"          oldbalanceDest,",
				"          newbalanceDest,",
				"          isFlaggedFraud",
				"     )) ~> PaysimSQL"
			]
		}
	}
}